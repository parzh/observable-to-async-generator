{
  "name": "observable-to-async-generator",
  "description": "Convert an observable to ES6 async generator.",
  "version": "1.0.0-rc",
  "author": "Dima Parzhitsky <parzhitsky@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parzhitsky/observable-to-async-generator.git"
  },
  "bugs": {
    "url": "https://github.com/parzhitsky/observable-to-async-generator/issues"
  },
  "homepage": "https://github.com/parzhitsky/observable-to-async-generator#readme",
  "keywords": [
    "*",
    "async",
    "await",
    "convert",
    "create",
    "function*",
    "function *",
    "generator",
    "iterable",
    "iterator",
    "observable",
    "otag",
    "yield",
    "yield*",
    "yield *"
  ],
  "files": [
    "/dist",
    "!/dist/**/*.{spec,test}.*",
    "/README.md"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "start": "node --require ts-node/register src",
    "prebuild": "rm -rf dist/*",
    "build": "tsc",
    "postbuild": "rm -rf dist/*.{spec,test}.*",
    "prepublishOnly": "npm test && npm run build"
  },
  "devDependencies": {
    "@types/jest": "26.0.4",
    "@types/node": "14.0.20",
    "jest": "26.1.0",
    "ts-jest": "26.1.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.6"
  },
  "peerDependencies": {
    "rxjs": "6.x.x"
  }
}
