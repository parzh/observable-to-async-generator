{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "outDir": "dist",
    "declaration": true,
    "sourceMap": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "skipLibCheck": true,

    "strict": true,
    "noUnusedLocals": true,
    "forceConsistentCasingInFileNames": true,
  },
}
